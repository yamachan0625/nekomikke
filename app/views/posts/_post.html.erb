<li class='post-card'>
        <div class='post-card__image'>
          <%= image_tag post.image.to_s%>
        </div>
        <div class='post-card__title'>
          <%= post.title%>
        </div>
        <div class='open-googlemap-btn'>
          <%= link_to "Googleマップで検索", "https://maps.google.co.jp/maps?q=loc:#{post.latitude},#{post.longitude}",target: "_blank" %>
        </div>
        <div class='post-card__btn'>
          <%= link_to "詳細"%>
        </div>
      </li>